{
    "name": "aporat/php_gearman_daemons",
    "description": "PHP Gearman Deamon Manager",
    "keywords": ["gearman", "workers", "queues", "deamons"],
    "type": "library",
    "homepage": "https://github.com/aporat/PHP-GearmanDaemons",
    "authors": [{
            "name": "Adar Porat",
            "email": "adar.porat@gmail.com",
            "homepage": "http://github.com/aporat"
        }
    ],
    "support": {
        "email": "adar.porat@gmail.com",
        "issues": "https://github.com/aporat/PHP-GearmanDaemons/issues"
    },
    "require": {
        "php": ">=5.3",
        "zendframework/zend-log": "2.1.5",
        "zendframework/zend-config": "2.1.5",
        "ext-pcntl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*@stable",
        "phpunit/php-invoker": ">=1.1.0,<1.2.0",
        "pdepend/pdepend": "dev-master",
        "phpmd/phpmd": "dev-master",
        "sebastian/phpcpd": "1.4.*@stable",
        "sebastian/finder-facade": "dev-master",
        "theseer/fdomdocument": "dev-master",
        "squizlabs/php_codesniffer": "1.4.*@stable",
        "apigen/apigen": "2.8.*@stable"
    },
    "suggest": {
        "ext-gearman": "*",
        "ext-proctitle": "Allows php-resque to rename the title of UNIX processes to show the status of a worker."
    },
    "autoload": {
        "psr-0": {
            "GearmanDaemons": "library/"
        }
    },
    "include-path": [
            "library/"
    ]
}
